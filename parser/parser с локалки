import requests
from bs4 import BeautifulSoup

url = 'http://localhost/wordpress/'

response = requests.get(url)
html = response.text
soup = BeautifulSoup(html, 'html.parser')
paragraphs = soup.find_all('p')

for paragraph in paragraphs:
    text = paragraph.text


    print(f'Текст параграфа: {text}')
